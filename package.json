{
  "name": "encrypted-db",
  "version": "0.0.15",
  "description": "Encrypted IndexedDB wrapper",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "http://github.com/carathorys/encrypted-db",
  "author": "carathorys",
  "license": "MIT",
  "preview": true,
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "watch": "tsc --watch",
    "doc:purge": "rimraf docs",
    "doc:run": "typedoc --theme markdown --out docs/ src/",
    "doc": "yarn doc:purge && yarn doc:run",
    "tslint": "tslint src/**/*.ts -t verbose",
    "lint": "yarn tslint",
    "test": "karma start",
    "release:build": "yarn build",
    "release:docs": "yarn doc",
    "release:copy": "node ./scripts/release.js",
    "prerelease": "yarn release:build && yarn release:copy",
    "release": "npm publish --cwd ./dist --tag next"
  },
  "nyc": {},
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "localforage": "^1.5.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.6.0",
    "@types/karma": "^1.7.1",
    "@types/localforage": "^0.0.34",
    "codecov": "^2.3.1",
    "fs-extra": "^4.0.2",
    "jasmine": "^2.8.0",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.1",
    "karma-typescript": "^3.0.8",
    "rimraf": "^2.6.2",
    "semver": "^5.4.1",
    "shelving-mock-indexeddb": "^1.0.4",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.11",
    "typescript": "^2.5.3"
  }
}
